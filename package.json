{
  "name": "frisbytestframework",
  "version": "1.0.0",
  "type": "module",
  "description": "Frisby Framework",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "frisby",
    "javascript",
    "api-automation",
    "test-framework"
  ],
  "jest": {
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Execution Report",
          "outputPath": "./report/test-report.html",
          "dateFormat": "dd/mm/yyyy HH:MM:SS"
        }
      ]
    ]
  },
  "author": "monika sharma",
  "license": "ISC",
  "devDependencies": {
    "frisby": "^2.1.3",
    "jest": "^26.4.2",
    "jest-html-reporter": "^3.2.0"
  }
}
